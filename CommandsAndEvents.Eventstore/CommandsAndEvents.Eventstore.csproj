<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <Authors>swmal</Authors>
    <Description>Plugin, publishing events on an Eventstore stream.</Description>
    <PackageLicenseUrl>https://github.com/swmal/CommandsAndEvents.Eventstore/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/swmal/CommandsAndEvents.Eventstore/</PackageProjectUrl>
    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
    <PackageTags>Eventstore, DDD</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommandsAndEvents" Version="1.0.1" />
    <PackageReference Include="EventStore.Client" Version="5.0.1" />
  </ItemGroup>

</Project>
